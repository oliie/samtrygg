					function removeAttachment(hash) {
						var arr = JSON.parse($("#attachmentsDeleted").val());
						arr.push(hash);
						$("#attachmentsDeleted").val(JSON.stringify(arr));
						$("#attachment-" + hash).remove();
					}
					$(function() {
						var arr = JSON.parse($("#attachmentsDeleted").val());
						for (var i = 0; i < arr.length; i++) {
							$("#attachment-" + arr[i]).remove();
						}
					});
