		<script>
			jQuery(document).ready(function($){
		        dzscal_init("#calendar_datepicker",{
		            design_transitionDesc: 'tooltipDef'
		            ,mode:'datepicker'
		            ,header_weekdayStyle: 'three'
		            ,design_transition: 'fade'
		            ,mode_datepicker_setTodayAsDefault: 'on'
		            ,start_weekday: 'Monday'
		        });

		        function dp1_event(arg){
		            var the_id = 'date_' + arg;
		            $(".event_info").hide();
		            $("#"+the_id).show();
		        }

		        var dp1 = document.getElementById('calendar_datepicker');
		        if(dp1){
		            dp1.arr_datepicker_events.push(dp1_event);
		        }

		        $(".event_info").hide();
		        $('.time-slot div input[type="radio"]').change(function(){
		          if($(this).is(":checked")) {
		              $(this).parents('div.row').addClass('selected');
		              $(this).parents('div.row').siblings().removeClass('selected');
		          } else {
		              $(this).parents('div.row').removeClass('selected');
		          }
		      });

		    });
		</script>

